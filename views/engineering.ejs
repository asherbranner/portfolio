<!DOCTYPE html>
<html>

<head>
  <%- include('head') %>
    <title>Engineering Design</title>
</head>

<body>
  <header>
    <%- include('header') %>
      <h1>Engineering Skills</h1>
  </header>

  <main>
    <!-- Grid layout for live tiles -->
    <div class="grid-container">

      <!-- Grid CAD -->
      <div class="grid-cad">
        <h2>Computer Aided Design</h2>
        <a href="#"><img src=/images/icons/cad.png alt="CAD"></a>
        <div>
          <progress id="cadexp" max="11" value="9"> 88%
          </progress>
        </div>
        <span class="progress-text">9 Years Experience</span>
        <div>
          <progress id="caddd" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">Drafting & Dimensioning</span>
        <div>
          <progress id="cad3d" max="100" value="95"> 95%
          </progress>
        </div>
        <span class="progress-text">3D Modeling</span>
        <div>
          <progress id="cadasm" max="100" value="90"> %
          </progress>
        </div>
        <span class="progress-text">Assembly Modeling</span>
        <div>
          <progress id="cadaa" max="100" value="75"> 70%
          </progress>
        </div>
        <span class="progress-text">Assembly Animation</span>
        <div>
          <progress id="cadesa" max="100" value="70"> 70%
          </progress>
        </div>
        <span class="progress-text">Engineering & Stress Analysis</span>
      </div>

      <!-- Grid Electrical Design -->
      <div class="grid-elec">
        <h2>Electrical Design</h2>
        <a href="#"><img src=/images/icons/elec.png alt="Electrical"></a>
        <div>
          <progress id="elecexp" max="11" value="10"> 91%
          </progress>
        </div>
        <span class="progress-text">10 Years Experience</span>
        <div>
          <progress id="pyctrl" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">Electrical Schematics</span>
        <div>
          <progress id="pyvis" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">PCB Design</span>
        <div>
          <progress id="eleclvd" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">Low-Voltage Design</span>
        <div>
          <progress id="eleclv" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">LV Debug/Test</span>
        <div>
          <progress id="pysim" max="100" value="95"> 95%
          </progress>
        </div>
        <span class="progress-text">LV Simulation</span>
        <div>
          <progress id="pyml" max="100" value="85"> 85%
          </progress>
        </div>
        <span class="progress-text">High-Voltage Design</span>
        <div>
          <progress id="pyml" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">HV Simulation</span>
      </div>

      <!-- Grid Team Collaboration -->
      <div class="grid-teams">
        <h2>Team Collabortion</h2>
        <a href="#"><img src=/images/icons/team.png alt="Team Collaboration"></a>
        <div>
          <progress id="file" max="11" value="9"> ~82%
          </progress>
        </div>
        <span class="progress-text">9 Years Experience</span>
        <div>
          <progress id="file" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Graphical Programming</span>
        <div>
          <progress id="file" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">Controls Systems Programming</span>
        <div>
          <progress id="file" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">Data Acquisition</span>
        <div>
          <progress id="file" max="100" value="70"> 70%
          </progress>
        </div>
        <span class="progress-text">Data Visualization</span>
        <div>
          <progress id="file" max="100" value="65"> 65%
          </progress>
        </div>
        <span class="progress-text">Live Debugging</span>
      </div>

      <!-- Grid C-Suite -->
      <div class="grid-csuite">
        <h2>Object-Based C</h2>
        <a href="software/c"><img src=/images/logos/lang/c-suite.png alt="C-Suite"></a>
        <div>
          <progress id="file" max="110" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">9 Years Experience</span>
        <div>
          <progress id="file" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Pointers, Interupts, O-OP</span>
        <div>
          <progress id="file" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">Data Structures & Algorithms</span>
        <div>
          <progress id="file" max="100" value="75"> 75%
          </progress>
        </div>
        <span class="progress-text">Memory Management & Optimization</span>
        <div>
          <progress id="file" max="100" value="75"> 75%
          </progress>
        </div>
        <span class="progress-text">.NET Framework</span>
        <div>
          <progress id="file" max="100" value="70"> 70%
          </progress>
        </div>
        <span class="progress-text">Event-Driven Programming</span>
        <div>
          <progress id="file" max="100" value="65"> 65%
          </progress>
        </div>
        <span class="progress-text">ML Algorithms</span>
      </div>

  </main>

  <footer>
    <%- include('footer') %>
  </footer>
</body>

</html>