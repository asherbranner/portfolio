<!DOCTYPE html>
<html>

<head>
  <%- include('../const/head') %>
    <link rel="stylesheet" href="/css/tiles/engineering/tiles-elec.css">
    <title>Electrical Experience</title>
</head>

<body>
  <header>
    <%- include('../const/header') %>
      <h1>Electrical Experience</h1>
  </header>
  <main>
    <!-- Grid layout for live tiles -->
    <div class="grid-container">
      <!-- Grid Circuit Design -->
      <div class="grid-cdesign">
        <h2>Circuit Design</h2>
        <img src=/images/logos/programs/multisim.png alt="multisim">
        <p>Insert Text</p>
      </div>
      <!-- Grid Embedded Systems Design -->
      <div class="grid-esdesign">
        <h2>Embedded Systems Design</h2>
        <img src=/images/logos/programs/cadence.png alt="cadence">
        <p>Object-Based C</p>
        <div>
          <progress id="file" max="100" value="20"> 20%
          </progress>
        </div>
        <span class="progress-text">2 Years Experience</span>
        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">High Technical Knowledge</span>
      </div>
      <!-- Grid High Voltage Design -->
      <div class="grid-hvdesign">
        <h2>High-Voltage Circuit Design</h2>
        <img src=/images/logos/programs/simulink.png alt="simulink">
        <p>Object-Based C</p>
        <div>
          <progress id="file" max="100" value="20"> 20%
          </progress>
        </div>
        <span class="progress-text">2 Years Experience</span>
        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">High Technical Knowledge</span>
      </div>

      <!-- Add more tiles as needed... -->
    </div>

    <!-- CSS styles for grid layout -->
  </main>
  <footer>
    <%- include('../const/footer') %>
  </footer>
</body>

</html>