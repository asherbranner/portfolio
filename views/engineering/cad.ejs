        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
          </div>
        <span class="progress-text">Flow Chart Design</span>
        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">Pipe Welding Instuction</span>
        <div>
          <progress id="file" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Part Analysis</span>
      </div>
      <!-- Grid Solidworks -->
      <div class="grid-solidworks">
        <h2>Solidworks</h2>
        <img src="/images/logos/programs/solidworks.png" alt="solidworks">
        <div>
          <progress id="file" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">8 Years Experience</span>
        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">eDrawing Creation</span>
