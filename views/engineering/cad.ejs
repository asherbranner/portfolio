          <progress id="file" max="100" value="100"> 100%
          </progress>
          </div>
        <span class="progress-text">Part Design</span>
        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
        </div>
        <span class="progress-text">Assembly Design</span>
        <div>
          <progress id="file" max="100" value="100"> 100%
          </progress>
          </div>
        <span class="progress-text">Building Layout</span>
      </div>

      <!-- Add more tiles as needed... -->
    </div>
    <div class="fill"></div>
    <!-- CSS styles for grid layout -->
  </main>
  <footer>
    <%- include('../const/footer') %>
  </footer>
</body>

</html>