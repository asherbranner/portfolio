      <!-- Grid Autocad -->
      <div class="grid-autocad">
        <h2>Autodesk AutoCad</h2>
        <img src="/images/logos/programs/autocad.png" alt="autocad">
        <div>
          <progress id="file" max="100" value="50"> 50%
