  <main>
    <!-- Grid layout for live tiles -->
    <div class="grid-container">
      <!-- Grid Inventor -->
      <div class="grid-inventor">
        <h2>Autodesk Inventor</h2>
        <img src=/images/logos/programs/inventor.png alt="inventor">
