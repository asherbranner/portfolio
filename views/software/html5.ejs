  <main>
    <!-- Grid layout for live tiles -->
    <div class="grid-container">
      <!-- Grid Booking -->
      <div class="grid-booking">
        <h2>Vacation Rental Booking</h2>
        <img src=/images/projects/booking.png alt="booking">
