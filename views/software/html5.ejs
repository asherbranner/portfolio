      </div>
      <!-- Grid Menu -->
      <div class="grid-menu">
        <h2>Restaurant Websites</h2>
        <img src="/images/projects/menu.png" alt="menu">
        <p>insert</p>
        <div>
          <progress id="file" max="100" value="30"> 30%
          </progress>
        </div>
        <span class="progress-text">3 Years Experience</span>
        <div>
          <progress id="file" max="100" value="67"> 67%
          </progress>
        </div>
