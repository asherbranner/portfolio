<!DOCTYPE html>
<html>

<head>
  <%- include('../const/head') %>
  <link rel = "stylesheet" href = "/css/tiles/software/tiles-python.css">
    <title>Python Experience</title>
</head>

<body>
  <header>
    <%- include('../const/header') %>
      <h1>Python Experience</h1>
  </header>
  <main>
    <!-- Grid layout for live tiles -->
    <div class="grid-container">
      <!-- Grid Tello -->
      <div class="grid-tello">
        <h2>DJI Tello</h2>
        <div class="video-container">
          <video width= "100%" height="100%" loop muted playsinline autoplay disablePictureInPicture src=/videos/tellodrone.mp4 type="video/mp4"></video>
        </div>
        <h3>Robotics 2 Project</h3>
        <div>
          <progress id="pythac" max="100" value="95"> 95%
          </progress>
        </div>
        <span class="progress-text">Aeronautic Controls</span>
        <div>
          <progress id="pyamp" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Autonomous Motion Profiling</span>
        <div>
          <progress id="pyvisprof" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">Vison Profiling</span>
        <div>
          <progress id="pythml" max="100" value="75"> 75%
          </progress>
        </div>
        <span class="progress-text">EEG Machine Learning</span>
      </div>
      <!-- Grid Robotic Arm -->
      <div class="grid-roboarm">
        <h2>Robotic Arm</h2>
        <div class="video-container">
          <video width= "100%" height="100%" loop muted playsinline autoplay disablePictureInPicture src=/videos/roboarm.mp4 type="video/mp4"></video>
        </div>
        <h3>Robotics 1 Project</h3>
        <div>
          <progress id="file" max="100" value="95"> 95%
          </progress>
        </div>
        <span class="progress-text">Linear Models & Controls</span>
        <div>
          <progress id="file" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Bluetooth and Wi-Fi</span>
        <div>
          <progress id="file" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Autonomous Motion Profiling</span>
        <div>
          <progress id="file" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">Motion Tracking</span>
      </div>

      <!-- Grid Pipetool -->
      <div class="grid-pipetool">
        <h2>Cost & Labor Sim</h2>
        <div class="video-container">
          <video width= "100%" height="100%" loop muted playsinline autoplay disablePictureInPicture src=/videos/pipetool.mp4 type="video/mp4"></video>
        </div>
        <h3>Engineering and Production Pipetool</h3>
        <div>
          <progress id="file" max="100" value="90"> 90%
          </progress>
        </div>
        <span class="progress-text">Time-Cost Analysis</span>
        <div>
          <progress id="file" max="100" value="85"> 85%
          </progress>
        </div>
        <span class="progress-text">SQL DB Server</span>
        <div>
          <progress id="file" max="100" value="85"> 85%
          </progress>
        </div>
        <span class="progress-text">Data Extrapolation</span>
        <div>
          <progress id="file" max="100" value="80"> 80%
          </progress>
        </div>
        <span class="progress-text">Real-Time Part Tracking</span>
      </div>
      
      <!-- Add more tiles as needed... -->
    </div>

    <!-- CSS styles for grid layout -->
  </main>
  <footer>
    <%- include('../const/footer') %>
  </footer>
</body>

</html>